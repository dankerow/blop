generator client {
  provider = "prisma-client-js"
}

generator json {
  provider = "prisma-json-types-generator"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       String @id @unique
  language String @default("en")
}

model Guild {
  id      String @id @unique
  /// [GuildModules]
  modules Json
}
